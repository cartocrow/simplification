{
  "version": 2,
  "configurePresets": [
    {
      "name": "CommonLocalPaths",
      "hidden": true,
      "cacheVariables": {
        "Ipelib_INCLUDE_DIR": {
          "value": "<path to ipe-msvc/src/include>",
          "type": "PATH"
        },
        "CMAKE_MODULE_PATH": {
          "value": "<path to cartocrow/core/cmake>",
          "type": "PATH"
        }
      },
      "environment": {
        "VCPKG_ROOT": "<path to /vcpkg>"
      }
    },
    {
      "name": "Debug",
      "inherits": [ "_x64-Debug", "CommonLocalPaths" ],
      "cacheVariables": {
        "Ipelib_LIBRARY": "<path to x64 Debug compiled ipe-msvc.lib>",
        "CartoCrow_DIR": "<path to CartoCrow's install/Debug/lib/cmake/CartoCrow>"
      }
    },
    {
      "name": "Release",
      "inherits": [ "_x64-Release", "CommonLocalPaths" ],
      "cacheVariables": {
        "Ipelib_LIBRARY": "<path to x64 Release compiled ipe-msvc.lib>",
        "CartoCrow_DIR": "<path to CartoCrow's install/Release/lib/cmake/CartoCrow>"
      }
    },
    {
      "name": "ReleaseWithDebug",
      "inherits": [ "_x64-RelWithDebInfo", "CommonLocalPaths" ],
      "cacheVariables": {
        "Ipelib_LIBRARY": "<path to x64 Release compiled ipe-msvc.lib>",
        "CartoCrow_DIR": "<path to CartoCrow's install/ReleaseWithDebug/lib/cmake/CartoCrow>"
      },
    }
  ]
}